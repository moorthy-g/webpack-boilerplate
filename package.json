{
  "name": "static-webpack-boilterplate",
  "version": "1.1.0",
  "description": "A simple webpack boilterplate with webpack-dev-server, babel & less",
  "license": "WTFPL",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve",
    "start": "npm run build && cross-env NODE_ENV=production serve build -p 8000",
    "build": "cross-env NODE_ENV=production webpack build --progress",
    "analyze": "cross-env ANALYZE_BUNDLE=true npm run build",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.15.0",
    "@commitlint/config-conventional": "^13.1.0",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "commitizen": "^4.2.4",
    "commitlint": "^13.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "^10.0.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.0",
    "mini-css-extract-plugin": "^2.2.0",
    "normalize.css": "^8.0.1",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "sass": "^1.38.0",
    "sass-loader": "^12.1.0",
    "serve": "^12.0.0",
    "style-loader": "^3.2.1",
    "terser-webpack-plugin": "^5.1.4",
    "webpack": "^5.50.0",
    "webpack-assets-manifest": "^5.0.6",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^3.11.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {}
}
