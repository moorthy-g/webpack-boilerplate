{
  "name": "static-webpack-boilterplate",
  "version": "1.0.0",
  "description": "A simple webpack boilterplate with webpack-dev-server, babel & less",
  "license": "WTFPL",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack -p --progress",
    "analyze": "cross-env ANALYZE_BUNDLE=true npm run build",
    "build:server": "npm run build && cross-env NODE_ENV=production serve build -p 8000"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@commitlint/config-conventional": "^8.3.4",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.0.3",
    "commitlint": "^8.3.5",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "cz-conventional-changelog": "3.1.0",
    "dotenv": "^8.0.0",
    "es6-promise": "^4.2.8",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "normalize.css": "^8.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "serve": "^11.3.0",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.41.6",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  }
}
